{
  "name": "aursu-javalocal",
  "version": "0.1.0",
  "author": "aursu",
  "summary": "Java local installatio (Oracle)",
  "license": "Apache-2.0",
  "source": "https://github.com/aursu/puppet-javalocal",
  "dependencies": [
    {
      "name":"puppetlabs/java",
      "version_requirement": ">= 2.4.0 < 3.0.0"
    },
    {
      "name": "aursu/lsys",
      "version_requirement": ">= 0.1.1 < 1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6", "7"
