      "operatingsystemrelease": [
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ],
  "pdk-version": "1.5.0",
  "template-url": "https://github.com/puppetlabs/pdk-templates",
  "template-ref": "1.5.0-0-gd1b3eca"
